/* 转 */
.fa-spinner:before {
    content: "\f110";
}
.faspin {
    -webkit-animation: faspin 1s infinite linear;
    animation: faspin 1s infinite linear;
}
@keyframes faspin{
  0%{
    transform: rotate(0deg);
    -webkit-transform: rotate(0deg)
  }
  100%{
    transform: rotate(359deg);
    -webkit-transform: rotate(359deg)
  }
}
@-webkit-keyframes faspin{
  0%{
    transform: rotate(0deg);
    -webkit-transform: rotate(0deg)
  }
  100%{
    transform: rotate(359deg);
    -webkit-transform: rotate(359deg)
  }
}
