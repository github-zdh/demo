/* 遮罩层 */

/*  */

.mask {
  position: fixed;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
  z-index: 2018;
}

/*   */

.relative {
  position: relative;
}

.absolute {
  position: absolute;
}

.fixed {
  position: fixed;
}
.flex{
    display: flex;
  display: -webkit-flex;
  display: -webkit-inline-flex;
}
.flexCenter{
  justify-content: center;
  align-items: center;
}
.transition{
  transition: all .4s;
  -webkit-transition: all .4s;
  -moz-transition: all .4s;
  -ms-transition: all .4s;
  -o-transition: all .4s;
}

.clearfix:after {
  display: block;
  height: 0;
  content: '';
  clear: both;
  visibility: hidden;
}

.overflowHide{
  overflow: hidden;
}
