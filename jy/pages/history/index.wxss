.search {
  height: 100rpx;
  background: rgba(255, 107, 103, 1);
}

.searchContent {
  padding: 18rpx 104rpx 18rpx 18rpx;
}

.searchInput {
  background: rgba(255, 255, 255, 1);
  border-radius: 10rpx;
  height: 64rpx;
}

.searchInput input {
  line-height: 64rpx;
  height: 64rpx;
  font-size: 24rpx;
  margin-left: 50rpx;
}

.searchInput .icon {
  top: 16rpx;
  left: 16rpx;
  width: 32rpx;
  height: 32rpx;
  line-height: 32rpx;
  font-size: 24rpx;
}

.searchButton {
  top: 0;
  right: 0;
  width: 104rpx;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 24rpx;
  color: rgba(255, 255, 255, 1);
}

.history .title {
  height: 34rpx;
  font-size: 24rpx;
  color: rgba(51, 51, 51, 1);
  line-height: 34rpx;
  font-weight: 600;
  margin: 16rpx 0 0 32rpx;
}

.history .fa-trash-o {
  margin: 13rpx 30rpx 0 0;
  line-height: 34rpx;
  color: #999;
  font-size: 30rpx;
}

.history .list {
  margin: 16rpx 32rpx;
}

.history .list .li {
  height: 50rpx;
  line-height: 50rpx;
  background: rgba(238, 238, 238, 1);
  border-radius: 30px;
  padding: 0 30rpx;
  margin: 10rpx 10rpx 10rpx 0;
  font-size: 22rpx;
}
