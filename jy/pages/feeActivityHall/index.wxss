page {
  height: 100%;
}

.feeContent, .feeContent .bg {
  width: 100%;
  height: 100%;
  overflow: hidden;
}

.feeContent .bg image, .feeContent .content {
  width: 100%;
  height: 100%;
}

.feeContent .content {
  left: 0;
  top: 0;
}

.hall .storename {
  width: 563rpx;
  height: 117rpx;
  top: 74rpx;
  left: 50%;
  margin-left: -281rpx;
}

.hall .storename image {
  width: 100%;
  height: 100%;
}

.hall .storename text {
  display: block;
  line-height: 117rpx;
  font-size: 51rpx;
  padding:0 10rpx;
  margin-top: 10rpx;
  color: rgba(153, 70, 15, 1);
}

.userInfo {
  top: 235rpx;
  left: 0;
  min-width: 244rpx;
  max-width: 60%;
  min-height: 90rpx;
  border-radius: 0 244rpx 244rpx 0;
  background: rgba(255, 255, 255, 0.3);
  display: flex;
  justify-content: start;
  align-items: center;
}

.userInfo .logo, .userInfo .logo image {
  width: 75rpx;
  height: 75rpx;
  overflow: hidden;
  border-radius: 50%;
}

.userInfo .logo {
  margin-left: 6rpx;
}

.userInfo .info {
  padding: 10rpx 20rpx 10rpx 6rpx;
  line-height: 33rpx;
  font-size: 22rpx;
  /* color:#000000; */
  color: rgba(255, 255, 255, 1);
}

.timeout {
  bottom: 66rpx;
  right: 13rpx;
  width:570rpx;
  height:250rpx;
}

.tiemoutContent {
  width: 100%;
  height: 100%;
}

.timeoutbg, .timeoutbg image {
  width: 100%;
  height: 100%;
}

.tiemoutContent .time {
  font-size: 100rpx;
  color: rgba(89, 46, 0, 1);
  line-height: 150rpx;
  top: 34rpx;
  right: 44rpx;
  transform: rotate(-5deg);
  -webkit-transform: rotate(-5deg);
}

.tiemoutContent .start {
  width: 60rpx;
  bottom: 90rpx;
  left: 44rpx;
  font-size: 20rpx;
  color: rgba(89, 46, 0, 1);
  line-height: 23rpx;
  transform: rotate(-15deg);
  -webkit-transform: rotate(-15deg);
}

.tiemoutContent .icon {
  bottom: 51rpx;
  left: 77rpx;
  width: 37rpx;
  height: 32rpx;
  transform: rotate(-7deg);
  -webkit-transform: rotate(-7deg);
}

.tiemoutContent .icon image {
  width: 100%;
  height: 100%;
  vertical-align: top;
}

.listContent {
  width: 690rpx;
  height: calc(100vh - 100rpx);
  top: 50rpx;
  /* height: calc( 100vh - 160rpx ); *//* top:80rpx; */
  left: 30rpx;
}

.listContent .bg, .listContent .bg image {
  width: 100%;
  height: 100%;
}

.listContent .tap {
  top: 32rpx;
  left: 0;
  height: 100rpx;
  width: 100%;
  display: flex;
  display: -webkit-flex;
  justify-content: center;
  align-items: center;
  flex: 1;
  /* background:pink; */
}

.listContent .tap view {
  font-size: 30rpx;
  border-radius: 300rpx;
  font-weight: 600;
  color: rgba(79, 0, 2, 1);
  min-width: 250rpx;
  padding: 10rpx;
  margin: 6rpx;
}

.listContent .tap view.active {
  color: RGBA(247, 192, 91, 1);
  background: #4f0002;
  box-shadow: 0rpx 6rpx 6rpx #0e0100 inset;
}

.listContent .tap view text {
  font-weight: 100;
}

.listContent .myInfo {
  width: 100%;
  bottom: 60rpx;
  left: 0;
  display: flex;
  display: -webkit-flex;
  justify-content: center;
  align-items: center;
}

.myInfoContent {
  max-width: 70%;
  border: 6rpx solid rgba(47, 5, 26, 1);
  border-radius: 30rpx;
  overflow: hidden;
  background: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
  box-shadow: inset 0 0 30rpx rgba(47, 5, 26, 1);
}

.myInfoContent .namelucky {
  padding: 0 30rpx;
  border-radius: 20rpx 0 0 20rpx;
  /* border:1rpx solid #ffffff; */
  background: #b8287c;
  height: 88rpx;
}

.myInfoContent .namelucky .name {
  font-size: 28rpx;
  color: rgba(255, 255, 255, 1);
  line-height: 50rpx;
  height: 50rpx;
  overflow: hidden;
}

.myInfoContent .namelucky .lucky {
  font-size: 20rpx;
  color: rgba(255, 255, 255, 1);
}

.myInfo .logo, .myInfo .logo image {
  width: 88rpx;
  height: 88rpx;
}

.onlinename {
  width: 86%;
  height: calc(100% - 330rpx);
  left: 7%;
  top: 132rpx;
  background: #4f0002;
  border: 1rpx solid #fff;
  box-shadow: 0 0 10rpx 0rpx #fff;
}

.onlinenameList {
  width: 100%;
  height: 100%;
}

.onlinenameList .storename {
  font-size: 36rpx;
  padding: 0 32rpx;
  height: 46px;
  line-height: 46px;
  background: #b91e29;
  color: rgba(255, 255, 255, 1);
  box-shadow: 0 0 30rpx 0 #701218 inset;
}

.onlinenameList .list {
  height: calc(100% - 46px);
  /* overflow: hidden; */
}

.onlinenameList .arrows {
  top: calc((100% + 46px)/2);
  left: -11vw;
  width: 100.6vw;
  margin-top: -30px;
  height: 60px;
  background: rgba(255, 255, 255, 0.1);
  z-index: 1000;
  justify-content: space-between;
  align-items: center;
}

.leftArrows, .rightArrows {
  width: 60px;
  height: 60px;
}

.rightArrows {
  transform: rotate(180deg);
}

.overflow {
  overflow: hidden;
}

.onlinenameList .list .li {
  border-bottom: 3px solid #ff357a;
  padding: 7px 16px;
  height: 43px;
}

.onlinenameList .list .lilogo, .onlinenameList .list .lilogo image {
  width: 43px;
  height: 43px;
}

.onlinenameList .list .usernamelucky .name {
  margin-top: 2rpx;
  font-size: 28rpx;
  color: rgb(218, 218, 152);
}

.onlinenameList .list .usernamelucky {
  overflow: hidden;
  padding-left: 24rpx;
}

.onlinenameList .list .lcukynum {
  height: 34rpx;
  line-height: 34rpx;
  padding: 0 6rpx;
  background: rgba(255, 255, 255, 0.2);
  border-radius: 32rpx;
  font-size: 20rpx;
  color: #f1f1f1;
  width: 90rpx;
  text-align: center;
  margin-top: 10rpx;
  display: inline-block;
}

.onlinenameList .list .lcukynum image {
  width: 24rpx;
  height: 24rpx;
  margin-top: 5rpx;
  margin-right: 5rpx;
  vertical-align: top;
}

.onlinenameList .list .li .num {
  bottom: -3px;
  right: 0;
  padding: 10rpx;
  height: 40rpx;
  line-height: 40rpx;
  min-width: 40rpx;
  color: rgba(255, 255, 255, 1);
  background: #ff357a;
  box-shadow: 0 0 16rpx #701218 inset;
}

.onlinenameList .list .li .freemealProduct {
  right: 32rpx;
  width: 6em;
  top: 50%;
  margin-top: -17px;
  height: 34px;
  line-height: 34px;
  color: rgba(255, 255, 0, 1);
  font-size: 36rpx;
}

.popup {
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.5);
  justify-content: center;
  align-items: center;
  /* background:  */
}

.countDownNun {
  width: 360rpx;
  height: 320rpx;
}

.popup .content {
  width: 750rpx;
  height: 750rpx;
  color: #fff;
}

.popup .content image.bgImg {
  width: 100%;
  height: 100%;
}

.popup .winInfo {
  top: 0;
  left: 50%;
  transform: translateX(-50%);
  -webkit-transform: translateX(-50%);
}

.popup .winInfo .title {
  font-size: 50rpx;
}

.popup .winInfo .usernametel {
  padding: 20rpx 0;
  font-size: 26rpx;
  white-space: nowrap;
}

.popup .winInfo .winProduct {
  width: 450rpx;
  height: 422rpx;
  margin:0 auto;
}

.popup .winInfo .goodsInfo {
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}

.popup .winInfo .goodsInfo image {
  margin-top: 66rpx;
  width: 122rpx;
  height: 122rpx;
  border-radius: 50%;
}

.popup .winInfo .goodsInfo .goodsname {
  margin-top: 36rpx;
}

.popup .winInfo .otherStore {
  font-size: 28rpx;
  left: 0;
  bottom: 0;
  color: rgba(53, 36, 4, 1);
  width: 100%;
  line-height: 100rpx;
}
.usernamelucky .distance{
  color:rgba(255,255,255,0.4);
  margin-left:10px;
}
.fa-close{
  z-index: 1000;
  top:20rpx;
  right:20rpx;
}
