.suggest_content {
  height: 200rpx;
}

.suggest_content_text {
  width: 90%;
  height: 200rpx;
  margin-left: 40rpx;
  margin-top: 10rpx;
  font-size: 24rpx;
}

.suggest_split {
  width: 100%;
  height: 8px;
  background: #f8f8f8;
}

.suggest_tel {
  height: 80rpx;
  margin-left: 40rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.suggest_tel_title {
  font-size: 30rpx;
  font-weight: 500;
}

.suggest_tel_text {
  font-size: 24rpx;
  margin-left: 30rpx;
  width: 500rpx;
}

.suggest_button {
  padding-top: 50rpx;
  height: 800rpx;
  background: #f8f8f8;
}

.suggest_button_save {
  color: #fff;
  background: #ff6b67;
  width: 90%;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 30rpx;
  font-weight: 500;
}

.photo_view {
  height: 200rpx;
  display: flex;
  align-items: center;
}

.each_photo_view {
  height: 180rpx;
  width: 180rpx;
  border: 2rpx dashed gray;
  margin-left: 40rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.add_photo_icon {
  height: 50rpx;
  width: 50rpx;
}

.choose_photo_view {
  height: 180rpx;
  width: 180rpx;
  margin-left: 40rpx;
  position: relative;
}

.choose_photo {
  height: 100%;
  width: 100%;
}

.deleteImg {
  width: 50rpx;
  height: 50rpx;
  z-index: 2;
  top: 0;
  right: 0;
  position: absolute;
}
