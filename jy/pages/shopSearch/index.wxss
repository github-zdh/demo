.search {
  height: 100rpx;
  background: rgba(255, 107, 103, 1);
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
}

.searchContent {
  padding: 18rpx 104rpx 18rpx 18rpx;
}

.searchInput {
  background: rgba(255, 255, 255, 1);
  border-radius: 10rpx;
  height: 64rpx;
}

.searchInput input {
  height: 64rpx;
  line-height: 64rpx;
  font-size: 24rpx;
  margin-left: 50rpx;
}

.searchInput .icon {
  top: 20rpx;
  left: 16rpx;
  width: 32rpx;
  height: 32rpx;
  line-height: 32rpx;
  font-size: 26rpx;
}

.searchButton {
  top: 0;
  right: 0;
  width: 104rpx;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 24rpx;
  color: rgba(255, 255, 255, 1);
}

.store_list {
  margin-bottom: 32rpx;
  margin-top: 100rpx;
}

/*  */

.store_list .store_logo, .store_list .store_logo image {
  width: 72rpx;
  height: 72rpx;
}

.store_list .li {
  padding: 16rpx 32rpx;
  border-bottom: 32rpx solid rgba(245, 245, 245, 1);
}

.store_list .store_content {
  overflow: hidden;
  padding-left: 18rpx;
}

.store_list .store_content .store_name {
  height: 34rpx;
  line-height: 34rpx;
  font-size: 26rpx;
  color: rgba(0, 0, 0, 1);
}

.store_list .store_content .store_name .name {
  width: 70%;
}

.store_list .store_content .store_name .tiemdistance {
  font-size: 20rpx;
  color: rgba(153, 153, 153, 1);
}

.store_list .store_content .store_volume {
  font-size: 22rpx;
  color: rgba(153, 153, 153, 1);
  line-height: 30rpx;
  margin-top: 6rpx;
}

.goods_list {
  margin-top: 32rpx;
}

.goods_logo, .goods_logo image {
  width: 160rpx;
  height: 160rpx;
}

.goods_content {
  overflow: hidden;
  padding-left: 32rpx;
}

.goods_content .goods_name {
  height: 40rpx;
  font-size: 28rpx;
  color: rgba(0, 0, 0, 1);
  line-height: 40rpx;
}

.goods_content .goods_volume {
  font-size: 20rpx;
  color: rgba(153, 153, 153, 1);
  line-height: 28rpx;
  min-height: 66rpx;
  margin-top: 8rpx;
}

.goods_content .howmoeny {
  margin-top: 4rpx;
  font-size: 32rpx;
  color: rgba(255, 107, 103, 1);
  line-height: 44rpx;
}

.seamore {
  margin-top: 32rpx;
  /* margin-left: 85rpx; */
  height: 28rpx;
  font-size: 20rpx;
  color: rgba(51, 51, 51, 1);
  line-height: 28rpx;
}

.fa-angle-up-after, fa-angle-down-after {
  padding-right: 10rpx;
}
